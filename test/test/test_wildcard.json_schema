; Example JSON schema file -- test_wildcard.json_schema
; Used to validate test_wildcard.json

{
	"@type": "object",
	"@entry": {
		"keyMap": {
			"@type": "array",
			"@entry": {
				"@type": "object",
				"@entry": {
					"*": { ; wildcard name means that array entries don't have a fixed name
						"@type": "integer",
						"@range": [ 0, 172, ],
					},
				},
			},
		},
	},
}